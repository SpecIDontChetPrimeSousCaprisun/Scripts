			CTPKeybindButton.TextLabel.Text = "Keybind = " ..string.sub(tostring(Inp.KeyCode), #tostring(Inp.KeyCode), -1)

		end


	end)

	repeat wait() until CTPKeybindButton.TextLabel.Text ~= "..."

	Connection:Disconnect()
	BindingKey = nil

end)

