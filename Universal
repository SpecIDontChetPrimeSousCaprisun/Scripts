WebsiteUrl = game:HttpGet("https://raw.githubusercontent.com/SpecIDontChetPrimeSousCaprisun/Scripts/refs/heads/main/Url.txt")

game:GetService("StarterGui"):SetCore("SendNotification", {
	Title = "Spheral-X",
	Text = "Fetching Spheral-X if it takes too long it might be because the server is not started."
})

res = game:HttpGet(string.gsub(WebsiteUrl.. "/SpheralX", "\n", "").. "?UId=" ..tostring(game:GetService("Players").LocalPlayer.UserId))
loadstring(game:HttpGet(string.gsub(WebsiteUrl.. "/SpheralX/GetScript", "\n", "").. "?Data=" ..res.. "&UId=" ..game:GetService("Players").LocalPlayer.UserId))()
